{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": false,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"baseUrl": ".",
		"paths": {
			"@components/*": ["components/*"],
			"@models/*": ["models/*"],
			"@styles/*": ["styles/*"]
		}
	},
	"include": [
		"types/**/*.ts",
		"next-env.d.ts",
		"svg.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"utils/database.ts",
		"app/profile"
	],
	"exclude": ["node_modules"]
}
